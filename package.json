    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "supabase": "^1.106.1",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.0"
  },
  "type": "module"
}
