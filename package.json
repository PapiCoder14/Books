  },
  "dependencies": {
    "@supabase/auth-helpers-sveltekit": "^0.10.3",
    "@supabase/supabase-js": "^2.38.2",
    "@vime/core": "^5.4.1",
    "@vime/svelte": "^5.4.1",
    "bits-ui": "^0.5.7",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "formsnap": "^0.2.0",
    "lucide-svelte": "^0.287.0",
    "sveltekit-superforms": "^1.8.0",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.14",
    "ui": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/kit": "latest",
    "@types/detect-port": "^1.3.4",
